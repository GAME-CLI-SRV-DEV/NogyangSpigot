+
 dependencies {
-    implementation(project(":paper-api"))
+    implementation(project(":NogyangSpigot-api"))
     // Paper start
+       +       // NogyangSpigot Start
+          compileOnly("org.geysermc.cumulus:cumulus:2.0.0-SNAPSHOT")
